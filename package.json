{
  "name": "atlas",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "start": "vite",
    "build": "node --max-old-space-size=8192 ./node_modules/.bin/vite build",
    "preview": "vite preview",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --color",
    "lint:fix": "eslint '*/**/*.{js,ts,tsx}' --fix",
    "lint:style": "stylelint 'src/**/*.{ts,tsx}'",
    "test": "jest --passWithNoTests --no-cache",
    "test:ci": "yarn test --coverage --noStackTrace --silent --ci --runInBand",
    "watch": "yarn test --watch",
    "prepare": "husky install",
    "deploy": "aws s3 sync dist/ s3://atlas-oportunidades --acl public-read"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.19.6",
